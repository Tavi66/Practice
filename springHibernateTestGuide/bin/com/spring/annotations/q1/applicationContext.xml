<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="=
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd">
      <bean id="ContactDetail" class="com.spring.ContactDetail" autowire="byType">
                   <property name="mobileNumber" value="9876543210"></property>
                   <property name="alternateMobileNumber" value="1234567890"></property>
                   <property name="landlineNumber" value = "22536458"></property>
                   <property name="email" value="me@gmail.com"></property>
      </bean>
      <bean id="Address" class="com.spring.Address">
                   <property name="line1" value="EL City"></property>
                   <property name="line2" value="Paris Corner"></property>
                   <property name="city" value="SLA Lake"></property>
                   <property name="pincode" value="641037"></property>
      </bean>
</beans>